{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/SmarterBalanced.SampleItems.Web/SmarterBalanced.SampleItems.Web.csproj"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "webpack-clean",
            "type": "shell",
            "isBackground": true,
            "command": "npm",
            "args": [
                 "run",
                 "clean",
                "--prefix",
                "SmarterBalanced.SampleItems.Web/"
            ],
            "problemMatcher": []
        }
    ]
}