  "scripts": {
    "install": "npm run build",
    "build": "npm run webpack-vendor && npm run webpack",
    "build:prod": "npm run webpack-vendor:prod && npm run webpack:prod",
    "webpack": "./node_modules/.bin/cross-env NODE_ENV=development webpack --config webpack.config.js --env.dev",
    "webpack:prod": "./node_modules/.bin/cross-env NODE_ENV=production webpack --config webpack.config.js --env.prod ",
    "webpack-vendor": "./node_modules/.bin/cross-env NODE_ENV=development webpack --config webpack.config.vendor.js --env.dev",
    "webpack-vendor:prod": "./node_modules/.bin/cross-env NODE_ENV=production webpack --config webpack.config.vendor.js --env.prod",
    "clean": "rm -rf ./wwwroot/dist ./wwwroot/Assets ?"
  },
